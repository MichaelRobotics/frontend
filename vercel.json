{
  "redirects": [
    { "source": "/", "destination": "/landing-page.html", "permanent": true }
  ],
  "cleanUrls": true,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static",
      "config": {
        "buildCommand": "echo 'MY_ENV_VAR: $MY_ENV_VAR'; echo 'ANOTHER_ENV_VAR: $ANOTHER_ENV_VAR'; true"
      }
    }
  ]
}